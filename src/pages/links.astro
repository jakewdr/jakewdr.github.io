            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());
            gtag('config', 'G-18TEL9N92D');
        </script>
	</head>
	<body>
		<Navigation />
		<h1>{pageTitle}</h1>
		<div class="big-text">
			<ul>
				<li><b><a href="https://www.instagram.com/jakewdr/">[Instagram]</a></b></li>
